                  <span class="input-group-addon"><i class="fa fa-users" aria-hidden="true"></i></span>
                  <select required="required" class="form-control" id="grupo" name="grupo">
                    <option value="default" selected disabled hidden>--Seleccione--</option>
                    <option value="Maternal I">Maternal I</option>
                    <option value="Maternal II">Maternal II</option>
                    <option value="Maternal III">Maternal III</option>
                  </select>
                </div>
              </div>
            </div>
            <div class="form-group">
              <label for="nombreNino" class="cols-sm-2 control-label">Nombre del niño(a):</label>
              <div class="cols-sm-10">
