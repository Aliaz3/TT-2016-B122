      <div class="col-xs-6 col-md-offset-3">
      <form class="" method="post" action="/Trabajosocial/Ingresoet">
            <div class="col-xs-8 form-group">
              <label for="cicloEsc" class="cols-sm-2 control-label">Ciclo Escolar:</label>
              <div class="cols-sm-10">
                <div class="input-group">
                  <span class="input-group-addon"><i class="fa fa-home" aria-hidden="true"></i></i></span>
                  <input type="text" class="form-control" name="cicloEsc" id="cicloEsc"  placeholder="Ciclo Escolar"/>
                </div>
              </div>
            </div>
            <div class="form-group">
              <label for="hora" class="cols-sm-2 control-label">Hora:</label>
              <div class="cols-sm-10">
                <div class="input-group">
                  <span class="input-group-addon"><i class="fa fa-clock-o" aria-hidden="true"></i></span>
                  <input type="text" required="required"  class="form-control" name="hora" id="hora"  placeholder="00:00 hrs"/>
                </div>
              </div>
            </div>
            <div class="col-xs-4 form-group">
              <label for="grupo" class="cols-sm-2 control-label">Grupo:</label>
